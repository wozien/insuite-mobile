{
  "name": "wx-insuite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:uat": "vite build --mode uat",
    "build:dev": "vite build --mode development",
    "serve": "vite preview",
    "lint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "yarn lint",
      "prettier --parser=typescript --write"
    ],
    "*.vue": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@vant/use": "^1.0.2",
    "axios": "^0.21.0",
    "bpmn-js": "^7.5.0",
    "lodash-es": "^4.17.21",
    "qs": "^6.10.1",
    "url": "^0.11.0",
    "vant": "^3.0.0",
    "vue": "^3.0.2",
    "vue-router": "^4.0.2",
    "vuex": "^4.0.0",
    "vxe-table": "^4.0.1",
    "xe-utils": "^3.0.3"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.0",
    "chalk": "^4.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^3.12.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "~3.9.3",
    "vconsole": "^3.4.0",
    "vite": "^2.0.5",
    "vite-plugin-style-import": "^0.8.1",
    "vue-tsc": "^0.0.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
